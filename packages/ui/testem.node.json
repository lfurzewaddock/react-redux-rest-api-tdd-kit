    "Node": {
      "command": "NODE_ENV=development node_modules/.bin/tape --require esm test/jsdom.environment.js test/unit/{,**/}/*.test.js",
      "protocol": "tap"
    }
  },
  "launch_in_dev": ["Node"]
}
