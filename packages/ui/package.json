{
  "name": "react-redux-rest-api-tdd-kit",
  "version": "1.0.0",
  "description": "React Redux REST API TDD Kit",
  "main": "index.js",
  "scripts": {
    "ui:tdd": "node_modules/.bin/testem -f testem.node.json",
    "ui:test": "NODE_ENV=development node_modules/.bin/tape --require esm test/jsdom.environment.js test/unit/{,**/}/*.test.js",
    "ui:test:debug": "NODE_ENV=debug node --nolazy --inspect-brk=5858 node_modules/tape/bin/tape --require esm test/jsdom.environment.js test/unit/{,**/}/*.test.js",
    "ui:start:dev": "webpack-dev-server --mode development --config webpack/dev --open",
    "ui:build:test:node": "BABEL_ENV=testIntegration node_modules/.bin/webpack --mode development --config ./webpack/test.node",
    "ui:integration:test": "pnpm run ui:build:test:node && node_modules/.bin/tape --require ./test/integration/jsdom-setup ../../build/ui/test_build_node/app.bundle.js | node_modules/.bin/tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfurzewaddock/react-redux-rest-api-tdd-kit"
  },
