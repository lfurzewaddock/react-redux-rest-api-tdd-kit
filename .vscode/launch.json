  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Debug UI Tests: Remote",
      "address": "localhost",
      "port": 5858,
      "localRoot": "${workspaceFolder}/packages/ui",
      "remoteRoot": "${workspaceFolder}/packages/ui"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome to Degug UI: Remote",
      "url": "http://localhost:8080",
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}/packages/ui"
    }
  ]
}
